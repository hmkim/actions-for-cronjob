&lt;p&gt;&lt;a href=&quot;mailto:6405-17@DESKTOP-7P1CJH6&quot;&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;6405-17@DESKTOP-7P1CJH6&lt;/span&gt;&lt;/a&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt; MINGW64 ~/Desktop/새 폴더 (master|MERGING)&lt;/span&gt;&lt;br /&gt;&lt;strong&gt;&lt;u&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;$ git diff&lt;/span&gt;&lt;/u&gt;&lt;/strong&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;diff --cc B.txt&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;index 823ebaf,ce01362..0000000&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;--- a/B.txt&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;+++ b/B.txt&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;@@@ -1,1 -1,1 +1,5 @@@&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;- &amp;lt;B9&amp;gt;ٺ&amp;lt;B8&amp;gt;&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;++&amp;lt;&amp;lt;&amp;lt;&amp;lt;&amp;lt;&amp;lt;&amp;lt; HEAD&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;++&amp;lt;B9&amp;gt;ٺ&amp;lt;B8&amp;gt;&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;++=======&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;+ hello&lt;/span&gt;&lt;br /&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;++&amp;gt;&amp;gt;&amp;gt;&amp;gt;&amp;gt;&amp;gt;&amp;gt; mb&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;a href=&quot;mailto:6405-17@DESKTOP-7P1CJH6&quot;&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;6405-17@DESKTOP-7P1CJH6&lt;/span&gt;&lt;/a&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt; MINGW64 ~/Desktop/새 폴더 (master|MERGING)&lt;/span&gt;&lt;br /&gt;&lt;strong&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;$ git add .&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;
&lt;p&gt;&lt;a href=&quot;mailto:6405-17@DESKTOP-7P1CJH6&quot;&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;6405-17@DESKTOP-7P1CJH6&lt;/span&gt;&lt;/a&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt; MINGW64 ~/Desktop/새 폴더 (master|MERGING)&lt;/span&gt;&lt;br /&gt;&lt;strong&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;$ git commit -m &quot;B.txt correct final&quot;&lt;/span&gt;&lt;br /&gt;&lt;/strong&gt;&lt;span style=&quot;COLOR: #ff0000&quot;&gt;[master d46e21c] B.txt correct final&lt;/span&gt;&lt;/p&gt;
