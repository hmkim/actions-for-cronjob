</div><div>public class example : MonoBehaviour&nbsp;</div><div>{</div><div><span style="white-space:pre">	</span>public string url = "https://community.arm.com/cfs-file/__key/communityserver-blogs-components-weblogfiles/00-00-00-20-66/6180.mali_2D00_shield_2D00_2.png";</div><div><br />
</div><div><span style="white-space:pre">	</span>IEnumerator Start()&nbsp;</div><div><span style="white-space:pre">	</span>{</div><div><span style="white-space:pre">		</span>WWW www = new WWW(url);</div><div><span style="white-space:pre">		</span>yield return www;</div><div><span style="white-space:pre">		</span>Renderer renderer = GetComponent&lt;Renderer&gt;();</div><div><span style="white-space:pre">		</span>renderer.material.mainTexture = www.texture;</div><div><span style="white-space:pre">	</span>}</div><div>}</div></div><div><br />
</div><div><br />
</div><br/><br/>tag : <a href="/tag/유니티" rel="tag">유니티</a>,&nbsp;<a href="/tag/unity" rel="tag">unity</a>,&nbsp;<a href="/tag/url" rel="tag">url</a>,&nbsp;<a href="/tag/web" rel="tag">web</a>,&nbsp;<a href="/tag/image" rel="tag">image</a>			 ]]> 
